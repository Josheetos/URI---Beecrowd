import java.util.*;
public class Uri1071 {

	public static void main(String[] args) {
	Scanner input = new Scanner (System.in);
int X, Y;
X = input.nextInt();
Y= input.nextInt();
input.close();
int soma = 0;
if (X < Y) {
	for (int i = X + 1; i < Y; i++) {
		if (i % 2 != 0) {
			soma += i;
		}
		
	}
}else{
	for (int i = Y + 1; i < X; i++) {
		if (i % 2 != 0) {
			soma += i;
		}
	}
}
System.out.println(soma);

	}

}
