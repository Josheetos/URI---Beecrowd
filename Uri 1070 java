import java.io.IOException;
import java.util.Scanner;

public class Uri1070 {
	
    public static void main(String[] args) throws IOException {
        Scanner input = new Scanner(System.in);
        int x = input.nextInt();
        input.close();
        int cont = 0;
        while (cont < 6) {
        	if (x % 2 == 1) {
        		System.out.println(x);
        		cont++;
        	}
    		x++;
        }
    }
	
}
