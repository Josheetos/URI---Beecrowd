import java.io.IOException;
import java.util.Scanner;

public class Uri1067 {
	public static void main(String[] args) throws IOException {
		Scanner input = new Scanner(System.in);
		int A = input.nextInt();
		input.close();
		for (int i = 1; i <= A; i++) {
			if (i % 2 != 0) {
				System.out.printf("%d\n", i);
			}
		}
	}
}
